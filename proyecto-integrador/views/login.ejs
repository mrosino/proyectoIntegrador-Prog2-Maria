<%- include('./partials/header') %>

<body class="bodyLogin">

	<div class="form">

		<div class="form-panel one">
			<div class="form-header">
				<h1>Ingresar</h1>
				<h2>Ingresá tu e-mail y contraseña para acceder al sitio</h2>
			</div>
			<div class="form-content">
				<form>
					<div class="form-group">
						<label for="username">Correo Electronico </label>
						<input type="email" id="username" name="username" placeholder="email" required="required" />
					</div>
					<div class="form-group">
						<label for="password">Contraseña</label>
						<input type="password" id="password" name="password" placeholder="contraseña"
							required="required" />
					</div>
					<div class='form-group'>
						<label class="form-remember">
							<input type="checkbox" />Recordar
						</label><a class="form-recovery" href="#">¿Olvidaste tu contraseña?</a>
					</div>
					<div class="form-group">
						<button type="submit"><a href="/ramo/profile">Iniciar sesión</a></button>					</div>
				</form>
			</div>
		</div>

	</div>

	<br><br><br><br><br><br><br><br><br>



	<%- include('./partials/footer') %>
</body>
</html>